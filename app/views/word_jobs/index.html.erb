<p id="notice"><%= notice %></p>

<h1>Word Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Last word</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_jobs.each do |word_job| %>
      <tr>
        <td><%= word_job.last_word %></td>
        <td><%= link_to 'Show', word_job %></td>
        <td><%= link_to 'Edit', edit_word_job_path(word_job) %></td>
        <td><%= link_to 'Destroy', word_job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word Job', new_word_job_path %>
